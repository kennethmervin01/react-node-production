{
  "extends": ["prettier", "eslint:recommended"],
  "plugins": ["prettier", "import"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2015,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "error",
    "no-console": "off"
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  }
}
